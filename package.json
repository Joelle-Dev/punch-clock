{
  "name": "punch-clock",
  "version": "1.0.0",
  "description": "轻量级打卡记录应用",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build && mkdir -p dist && touch dist/.nojekyll",
    "build:docs": "vite build && touch dist/.nojekyll && cp icon-192.png.jpg icon-512.png.jpg dist/ 2>/dev/null || true && rm -rf docs && cp -r dist docs && touch docs/.nojekyll",
    "preview": "vite preview"
  },
  "keywords": [
    "punch",
    "clock",
    "attendance",
    "pwa"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vant/auto-import-resolver": "^1.3.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "unplugin-auto-import": "^0.18.0",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "vant": "^4.9.22",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  }
}
